        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ninthhomework.ScheduleDetailActivity"></activity>
        <receiver android:name="com.example.ninthhomework.ScheduleDetailActivity$AlarmReceiver" >
