                <action android:name="com.example.senior.ScheduleDetailActivity.AlarmReceiver" />
            </intent-filter>
        </receiver>
        <activity android:name="com.example.ninthhomework.ScheduleActivity" android:exported="true" />
        <activity android:name="com.example.ninthhomework.TabButtonActivity" android:exported="true" />
        <activity android:name="com.example.ninthhomework.MainActivity">

        </activity>
    </application>
    <uses-permission android:name="android.permission.VIBRATE" />
</manifest>